{
  "name": "json-reduce",
  "author": {
    "name": "Angelos Karagkiozidis",
    "email": "angelkar@gmail.com"
  },
  "publisher": "ang3lkar",
  "displayName": "JSON Reduce",
  "description": "Reduce large JSON objects to ease readability",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ang3lkar/json-reduce"
  },
  "type": "module",
  "engines": {
    "vscode": "^1.85.0"
  },
  "scripts": {
    "test": "node --test src/compact.test.js"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:json-reduce.run"],
  "main": "./index.js",
  "contributes": {
    "commands": [
      {
        "command": "json-reduce.run",
        "title": "JSON Reduce"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "JSON Reduce Configuration",
      "properties": {
        "jsonReduce.stringLimit": {
            "type": "number",
            "default": 300,
            "description": "JSON reduce: The maximum length of string values when reducing JSON."
        },
        "jsonReduce.arrayLimit": {
            "type": "number",
            "default": 3,
            "description": "JSON reduce: The maximum number of items in arrays when reducing JSON."
        }
      }
    }
  }
}
